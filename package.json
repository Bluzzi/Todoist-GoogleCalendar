{
  "scripts": {
    "start": "node -r @swc-node/register ./src/main.ts",
    "dev": "nodemon --exec node -r @swc-node/register ./src/main.ts",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "ts:check": "tsc --noEmit",
    "db:viewer": "prisma studio --port 3002",
    "db:migration:create": "prisma migrate dev",
    "db:migration:apply": "prisma migrate deploy",
    "db:reset": "prisma migrate reset",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@doist/todoist-api-typescript": "^3.0.2",
    "@googleapis/calendar": "^9.7.0",
    "@googleapis/people": "^3.0.7",
    "@hono/node-server": "^1.9.0",
    "@prisma/client": "^5.11.0",
    "croner": "^8.0.1",
    "dayjs": "^1.11.10",
    "dotenv": "^16.4.5",
    "google-auth-library": "^9.7.0",
    "hono": "^4.1.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@bluzzi/eslint-config": "^1.1.0",
    "@swc-node/register": "^1.6.8",
    "@swc/core": "^1.3.91",
    "@types/node": "^20.8.0",
    "eslint": "^8.50.0",
    "nodemon": "^3.0.1",
    "prisma": "^5.11.0",
    "typescript": "^5.2.2"
  },
  "eslintConfig": {
    "extends": "@bluzzi",
    "rules": {
      "camelcase": "off"
    }
  }
}
